	<import resource="conversion-service.xml" />
	
	<!-- Some Users -->
	
	
    
    
    <bean name="auditorUserAccount" class="acme.framework.entities.UserAccount">
        <property name="username" value="auditor" />
        <property name="password" value="auditor" />
        <property name="enabled" value="true" />
        <property name="identity">
            <bean class="acme.datatypes.UserIdentity">
                <property name="name" value="Claudio" />
                <property name="surname" value="Fernández" />
                <property name="email" value="clafer@acme.com" />
            </bean>
        </property>
        <property name="roles">
            <list>
                <ref bean="authenticatedAuditor" />
                <ref bean="auditor1" />
            </list>
        </property>
    </bean>

    <bean name="authenticatedAuditor" class="acme.framework.entities.Authenticated">
        <property name="userAccount" ref="auditorUserAccount" />
    </bean>

